/**
 * Theme function for REGON field.
 */
function theme_regon($variables) {
  $output = '';

  if (!empty($variables['element']['number'])) {
    $output .= '<div><span>' . $variables['element']['number'] . '</span></div>';
  }

  return $output;
}
